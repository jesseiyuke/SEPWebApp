@model Referees
@{
    ViewData["Title"] = "AddReferees";
}
<h3>Referees</h3>

<form method="post">
    <input asp-for="Id" hidden />
    <div class="col pb-2">
        <label class="col-sm-2 col-form-label fw-bold">Name</label>
        <label>@Model.Name</label>
    </div>
    <div class="col pb-2">
        <label class="col-sm-2 col-form-label fw-bold">Job Title</label>
        <label>@Model.JobTitle</label>
    </div>

    <div class="col pb-2">
        <label class="col-sm-2 col-form-label fw-bold">Insitution</label>
        <label>@Model.Insitution</label>
    </div>
        <div class="col pb-2">
        <label class="col-sm-2 col-form-label fw-bold">Cell</label>
        <label>@Model.Cell</label>
    </div>
    <div class="col pb-2">
        <label class="col-sm-2 col-form-label fw-bold">Email</label>
        <label>@Model.Email</label>
    </div>

    <br />
@*    <a href="/Employer/JobPost/Detail?id=@Model.Student." class="btn btn-secondary" style="width:150px">Back</a>*@

